{
  "name": "tailwindcss-landingpage-example",
  "version": "1.0.0",
  "private": true,
  "description": "Landing Page TailwindCSS Example",
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss css/tailwind.css -o public/build/tailwind.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss css/tailwind.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss css/tailwind.css -o public/build/tailwind.css"
  },
  "dependencies": {
    "tailwindcss": "^2.0.2",
    "autoprefixer": "^10.2.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^12.0.1"
  }
}
